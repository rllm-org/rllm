# Anthropic API Key
# Get your API key from: https://console.anthropic.com/
# ANTHROPIC_API_KEY=sk-ant-api03-...

# OpenAI API Key (optional, if you want to use OpenAI models)
OPENAI_API_KEY=sk-...
OPENAI_BASE_URL=https://api.openai.com/v1

# Other API Keys (if needed)
# GOOGLE_API_KEY=...
# AZURE_OPENAI_API_KEY=...

# Disable Tokenizers Parallelism (disable warning)
TOKENIZERS_PARALLELISM=false

FIRECRAWL_API_KEY=

# STRANDS DEBUG FLAG
RLLM_DEBUG_JSONL=0
RLLM_DEBUG_EVENTS=0

# Google Search API Configuration
GOOGLE_SEARCH_SECRET_KEY=your_actual_api_key_here
GOOGLE_SEARCH_ENGINE_ID=your_actual_engine_id_here